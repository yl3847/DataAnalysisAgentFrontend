{"ast":null,"code":"import { logs, pows } from './log';\nexport const d3LogNice = (a, b, _, base) => {\n  const shouldReflect = a < 0;\n  const log = logs(base, shouldReflect);\n  const pow = pows(base, shouldReflect);\n  const r = a > b;\n  const min = r ? b : a;\n  const max = r ? a : b;\n  const niceDomain = [pow(Math.floor(log(min))), pow(Math.ceil(log(max)))];\n  return r ? niceDomain.reverse() : niceDomain;\n};","map":{"version":3,"names":["logs","pows","d3LogNice","a","b","_","base","shouldReflect","log","pow","r","min","max","niceDomain","Math","floor","ceil","reverse"],"sources":["utils/d3-log-nice.ts"],"sourcesContent":[null],"mappings":"AACA,SAASA,IAAI,EAAEC,IAAI,QAAQ,OAAO;AAElC,OAAO,MAAMC,SAAS,GAAeA,CAACC,CAAC,EAAEC,CAAC,EAAEC,CAAC,EAAEC,IAAI,KAAI;EACrD,MAAMC,aAAa,GAAGJ,CAAC,GAAG,CAAC;EAC3B,MAAMK,GAAG,GAAGR,IAAI,CAACM,IAAI,EAAEC,aAAa,CAAC;EACrC,MAAME,GAAG,GAAGR,IAAI,CAACK,IAAI,EAAEC,aAAa,CAAC;EACrC,MAAMG,CAAC,GAAGP,CAAC,GAAGC,CAAC;EACf,MAAMO,GAAG,GAAGD,CAAC,GAAGN,CAAC,GAAGD,CAAC;EACrB,MAAMS,GAAG,GAAGF,CAAC,GAAGP,CAAC,GAAGC,CAAC;EACrB,MAAMS,UAAU,GAAG,CAACJ,GAAG,CAACK,IAAI,CAACC,KAAK,CAACP,GAAG,CAACG,GAAG,CAAC,CAAC,CAAC,EAAEF,GAAG,CAACK,IAAI,CAACE,IAAI,CAACR,GAAG,CAACI,GAAG,CAAC,CAAC,CAAC,CAAC;EACxE,OAAOF,CAAC,GAAGG,UAAU,CAACI,OAAO,EAAE,GAAGJ,UAAU;AAC9C,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}